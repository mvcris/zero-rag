{
  "compilerOptions": {
    "target": "es2016",    
    "module": "CommonJS",               
    "moduleResolution": "node",
    "esModuleInterop": true,                           
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "strictPropertyInitialization": false,
    "skipLibCheck": true,
    "outDir": "./dist"
  },
  "include": ["src/**/*.ts"]
}
